{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "*/30 * * * * *"
    },
    {
      "type": "eventHub",
      "name": "$return",
      "eventHubName": "%EventHubName%",
      "connection": "EventHubConnection",
      "direction": "out"
    }
  ]
}